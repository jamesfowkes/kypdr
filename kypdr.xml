<?xml version="1.0"?>

<board type="AStar32U4" name="kypdr">
    <info>
        Declares required ADL files for the kypdr board
    </info>
    
    <devices>
        <device name="Binary Output" type="Binary Output">
            <setting id="pins" value="5,4,3"/>
        </device>
    </devices>

    <parameters>
        <parameter name="Single Keypress" type="Boolean">
            <setting id="use_eeprom" value="true"/>
        </parameter>
    </parameters>

    <modules>
        <module>Task</module>
        <module>Debouncer</module>
    </modules>

    <custom_code>
        <file>kypdr.cpp</file>
    </custom_code>
    
    <adl protocol="url"></adl>
</board>
